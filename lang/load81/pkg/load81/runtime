			<dir name="dev"> <log/> <inline name="rtc">2000-01-01 00:00</inline> </dir>
			<fs label="src"/>
			<import overwrite="no">
				<tar name="examples.tar"/>
				<rom name="wave.lua" binary="no"/>
			</import>
		</vfs>
		<arg value="load81"/>
		<arg value="wave.lua"/>
	</config>

	<content>
		<rom label="vfs.lib.so"/>
